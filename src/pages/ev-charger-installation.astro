    transition: transform 0.2s ease;
  }

  .charger-card:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(0,0,0,0.1);
  }

  .charger-card.featured {
    border: 2px solid var(--brand);
    background: linear-gradient(135deg, #f0f9ff, #e0f2fe);
  }

  .specs {
    background: #f8fafc;
    padding: 1rem;
    border-radius: 8px;
    margin: 1rem 0;
  }

  .specs p {
    margin: 0.5rem 0;
    font-size: 0.9rem;
  }

  .price {
    font-weight: bold;
    color: var(--brand);
